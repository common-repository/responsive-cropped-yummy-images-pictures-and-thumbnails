{
  "name": "responsive-and-cropping-images",
  "version": "1.0.0",
  "description": "Responsive and cropping images Wordpress plugin",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:umd && yarn sass",
    "build:umd": "webpack --progress --mode=development",
    "build:umd:watch": "webpack --progress --colors --watch --mode=development",
    "build:umd:min": "webpack --progress --mode=production --config=webpack.live.config.js",
    "sass": "csscomb app/scss && webpack -p --progress --mode=production --config=webpack.sass.config.js",
    "sass:watch": "webpack -p --colors --progress --mode=production --watch --config=webpack.sass.config.js"
  },
  "author": "Konstantin Melnikov",
  "license": "default",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cropperjs": "^1.5.1",
    "jquery-cropper": "^1.0.0",
    "vue": "^2.5.17",
    "vue-loading-overlay": "^3.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "7",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^1.0.0",
    "csscomb": "^4.2.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.10.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-loader": "^15.4.0",
    "vue-template-compiler": "^2.5.17",
    "vue-template-loader": "^1.0.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
